[Builder]
SERVER_STATE_DIR=${BUILD_DIR}/update
BUNDLE_DIR=${BUILD_DIR}/mix-bundles
YUM_CONF=${BUILD_DIR}/.yum-mix.conf
CERT=${BUILD_DIR}/Swupd_Root.pem
VERSIONS_PATH=${BUILD_DIR}/
LOCAL_RPM_DIR=${BUILD_DIR}/rpms
LOCAL_REPO_DIR=${BUILD_DIR}/local

[swupd]
BUNDLE=os-core-update
CONTENTURL=${DSTREAM_DL_URL}/update
VERSIONURL=${DSTREAM_DL_URL}/update
FORMAT=1

[Server]
debuginfo_banned=true
debuginfo_lib=/usr/lib/debug/
debuginfo_src=/usr/src/debug/

[Mixer]
LOCAL_BUNDLE_DIR=${BUILD_DIR}/mix-bundles
